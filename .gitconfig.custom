[core]
  repositoryformatversion = 0
  filemode = true
  bare = false
  logallrefupdates = true
  ignorecase = false
  precomposeunicode = false
  autocrlf = false
  quotepath = false
  editor = vim
	autocrlf = input
[merge]
	ff = false
[push]
  default = current
[pull]
	ff = only
[alias]
	st = status
  sts = status -s
	co = checkout
	br = branch
	cm = commit
	df = diff
  lg = log --oneline --graph -10
  lgb = log --oneline --graph --branches -15
[color]
	ui = auto
